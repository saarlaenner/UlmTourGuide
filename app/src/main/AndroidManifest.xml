<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.ulmtourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BarsActivity"
            android:label="@string/bars"/>
        <activity android:name=".FamilyActivity"
            android:label="@string/family"/>
        <activity android:name=".RestaurantsActivity"
            android:label="@string/restaurants"/>
        <activity android:name=".SightsActivity"
            android:label="@string/sights"/>
    </application>

</manifest>